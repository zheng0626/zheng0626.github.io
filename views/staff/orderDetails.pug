extends ../partials/layoutSidebar.pug
block innerContent
  #orderDetail
    #order
      p Collection Num : #{order_trans.collectionNum}
      table
        thead
          tr
            td Product Name
            td Quantity
        for product in order_items
          tbody
            tr
              td=product.briefName
              td=product.quantity
      p Total : #{order.total}